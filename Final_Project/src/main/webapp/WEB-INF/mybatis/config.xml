		<setting name="jdbcTypeForNull" value="VARCHAR" />
	</settings>
	<typeAliases>
		<typeAlias type="net.edupoll.kr.entity.AccountVo"
			alias="account" />
		<typeAlias type="net.edupoll.kr.entity.MessageVo"
			alias="message" />
		<typeAlias type="net.edupoll.kr.entity.TimeCountVo"
			alias="timeCount" />
		<typeAlias type="net.edupoll.kr.entity.FreeBoardVo"
			alias="freeBoard" />
	</typeAliases>
</configuration> 

